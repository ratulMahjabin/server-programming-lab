<div class="right_col" role="main">
  <!-- top tiles -->
  <div class="col-md-12 col-sm-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>MathOlympiad<small>/ Participants' List</small></h2>
        <ul class="nav navbar-right panel_toolbox">
          <li>
            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
        <br />
        <% let success3= 'Participant has been selected succesfully'%> <% let
        success= 'Data has been deleted successfully!'%> <% let success2=
        'Payment completed succesfully'%> <% if (error.length>0) {if
        (error==success||success2||success3){%>
        <div class="alert alert-success"><%=error%></div>
        <%}else{%>
        <div class="alert alert-danger"><%=error%></div>
        <%}%> <%}%>

        <div class="row">
          <div class="col-sm-12">
            <div class="card-box table-responsive">
              <table
                id="datatable-responsive"
                class="table table-striped table-bordered nowrap"
                cellspacing="0"
                width="100%"
              >
                <thead>
                  <tr>
                    <!-- <th>ID</th> -->
                    <th>Name</th>
                    <th>Category</th>
                    <th>
                      Contact<br />
                      No.
                    </th>
                    <th>E-mail</th>
                    <th>Institute</th>
                    <th>T-Shirt</th>
                    <th>Total</th>
                    <th>Paid</th>
                    <th>Selected</th>
                    <th>
                      Payment<br />
                      Status
                    </th>
                    <th>Select</th>
                    <th>
                      Payment<br />
                      Done
                    </th>
                    <th>Edit</th>
                    <th>Delete</th>
                    <th>Verfied</th>
                    <th>Verify</th>
                    <th>HashValue</th>
                  </tr>
                </thead>
                <tbody>
                  <%if(participants.length>0){
                  participants.forEach(participant=>{%>
                  <tr>
                    <!-- <td><%=participant._id%></td> -->

                    <td><%=participant.name%></td>
                    <td><%=participant.category%></td>
                    <td><%=participant.contact%></td>
                    <td><%=participant.email%></td>
                    <td><%=participant.institution%></td>
                    <td><%=participant.tshirt%></td>
                    <td><%=participant.total%></td>
                    <td><%=participant.paid%></td>
                    <td>
                      <%if(participant.selected==true){%>
                      <p style="color: green">Yes</p>
                      <%}else{%>
                      <p style="color: brown">No</p>
                      <%}%>
                    </td>
                    <td>
                      <%if(participant.paid==participant.total){%>
                      <p style="color: green">Successful</p>
                      <%}else{%>
                      <p style="color: brown">Incomplete</p>
                      <%}%>
                    </td>
                    <td>
                      <% const selectURL= 'select/'+ participant._id %>
                      <a href="<%= selectURL %>"
                        ><i class="fa fa-check"></i
                        ><span class="fa fa-chevron"></span
                      ></a>
                    </td>
                    <td>
                      <% const paymentDoneURL= 'paymentDone/'+ participant._id
                      %>
                      <a href="<%= paymentDoneURL %>"
                        ><i class="fa fa-money"></i
                        ><span class="fa fa-chevron"></span
                      ></a>
                    </td>
                    <td>
                      <% const editURL= 'edit-participant/'+participant._id %>
                      <a href="<%= editURL%>"
                        ><i class="fa fa-pencil"></i
                        ><span class="fa fa-chevron"></span
                      ></a>
                    </td>

                    <td>
                      <% const deleteURL= 'delete/'+ participant._id %>
                      <a href="<%= deleteURL%>"
                        ><i class="fa fa-trash"></i
                        ><span class="fa fa-chevron"></span
                      ></a>
                    </td>
                    <td>
                      <%if(participant.verified == true){%>
                      <p style="color: cadetblue">Yes</p>
                      <%}else{%>
                      <p style="color: brown">No</p>
                      <%}%>
                    </td>
                    <td>
                      <% const verifyURL = "verify/" + participant._id %>
                      <a
                        href="<%= verifyURL %>"
                        data-toggle="tooltip"
                        title="Verify"
                      >
                        <i
                          style="color: rgb(7, 180, 94)"
                          class="glyphicon glyphicon-new-window"
                        ></i>
                      </a>
                    </td>
                    <td><%= participant.hashValue%></td>
                  </tr>
                  <% }) }%>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
